{
  "expo": {
    "name": "Sulimed",
    "slug": "app-sulimed",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/playstore-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playstore-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.appsulimed"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/playstore-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-sulimed-v2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      "expo-secure-store",
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true
        }
      }]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "92043172-216a-4be0-8cfd-cd2058b0e6ea"
      }
    }
  }
}
